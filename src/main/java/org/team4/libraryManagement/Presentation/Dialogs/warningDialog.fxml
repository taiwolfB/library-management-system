<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import com.jfoenix.controls.JFXDialog?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import com.jfoenix.controls.JFXTextField?>
<JFXDialog xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="org.team4.libraryManagement.Presentation.Dialogs.WarningDialogController"
           prefWidth="600.0"
           fx:id="root">
    <JFXDialogLayout>
        <heading>
            <Label styleClass="m-heading" fx:id="heading">Warning!</Label>
        </heading>
        <body>
            <Label fx:id="body"/>
        </body>
        <actions>
            <JFXButton styleClass="m-buttoncard" onAction="#closeDialog">CANCEL</JFXButton>
            <JFXButton styleClass="m-button" onAction="#saveDialog" fx:id="saveButton">CONTINUE</JFXButton>
        </actions>
    </JFXDialogLayout>
</JFXDialog>

